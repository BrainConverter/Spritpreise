{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "max-line-length": {
            "severity": "off"
        },
        "no-console": {
            "severity": "off",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "object-literal-sort-keys": false,
        "ordered-imports": true,
        "no-trailing-whitespace": true,
        "eofline": true,
        "semicolon": [true, "never"]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "dist/",
            "*.json",
            "**/*.json"
        ]
    }
}